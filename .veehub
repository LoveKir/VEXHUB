local TPTab = Window:CreateTab("FARM", nil) -- Title, Image

local checkpointsFolder = workspace:WaitForChild("Checkpoints") -- Ganti sesuai nama folder kamu

-- Fungsi teleport ke checkpoint
local function teleportToCheckpoint(cpName)
	local player = game.Players.LocalPlayer
	local character = player.Character or player.CharacterAdded:Wait()
	local hrp = character:WaitForChild("HumanoidRootPart")
	local checkpoint = checkpointsFolder:FindFirstChild(cpName)

	if checkpoint then
		hrp.CFrame = checkpoint.CFrame + Vector3.new(0, 2, 0)
		print("Teleported to:", cpName)
	else
		warn("Checkpoint", cpName, "tidak ditemukan di Workspace.")
	end
end

-- Cari checkpoint terdekat dengan posisi pemain
local function getNearestCheckpoint()
	local player = game.Players.LocalPlayer
	local character = player.Character or player.CharacterAdded:Wait()
	local hrp = character:WaitForChild("HumanoidRootPart")

	local nearestCP, nearestDist, nearestIndex = nil, math.huge, 0

	for i = 0, 11 do
		local cp = checkpointsFolder:FindFirstChild("CP" .. i)
		if cp then
			local dist = (cp.Position - hrp.Position).Magnitude
			if dist < nearestDist then
				nearestDist = dist
				nearestCP = cp
				nearestIndex = i
			end
		end
	end

	return nearestIndex, nearestCP
end


-- =====================================================
-- AUTO TELEPORT LOOP DARI CP SAAT INI SAMPAI CP11 → ULANG KE CP0
-- =====================================================

local autoTeleport = false

local AutoButton = TPTab:CreateButton({
	Name = "Start / Stop Auto Teleport (SLOW~)",
	Callback = function()
		autoTeleport = not autoTeleport

		if autoTeleport then
			print("Auto teleport loop dimulai...")

			task.spawn(function()
				while autoTeleport do
					-- Cari CP terdekat
					local startIndex = getNearestCheckpoint()
					print("Mulai dari CP" .. startIndex)

					-- Teleport dari CP sekarang sampai CP11
					for i = startIndex, 11 do
						if not autoTeleport then break end
						local cpName = "CP" .. i
						teleportToCheckpoint(cpName)
						task.wait(2)
					end

					-- Kembali ke CP0 setelah CP11
					if autoTeleport then
						for i = 0, startIndex - 1 do
							if not autoTeleport then break end
							local cpName = "CP" .. i
							teleportToCheckpoint(cpName)
							task.wait(2)
						end
					end
				end
			end)

		else
			print("Auto teleport loop dihentikan.")
		end
	end,
})

