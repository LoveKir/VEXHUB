local checkpointsFolder = workspace:WaitForChild("Checkpoints") -- Ganti sesuai nama folder kamu

-- Fungsi umum untuk teleport ke checkpoint
local function teleportToCheckpoint(cpName)
	local player = game.Players.LocalPlayer
	local character = player.Character or player.CharacterAdded:Wait()
	local hrp = character:WaitForChild("HumanoidRootPart")
	local checkpoint = checkpointsFolder:FindFirstChild(cpName)

	if checkpoint then
		-- Teleport dengan sedikit offset biar nggak nyangkut
		hrp.CFrame = checkpoint.CFrame + Vector3.new(0, 3, 0)
		print("Teleported to:", cpName)
	else
		warn("Checkpoint", cpName, "tidak ditemukan di Workspace.")
	end
end

